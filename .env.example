# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/proxy_server
MONGODB_TEST_URI=mongodb://localhost:27017/proxy_server_test

# JWT Authentication
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRE=24h
JWT_REFRESH_SECRET=your_refresh_token_secret
JWT_REFRESH_EXPIRE=7d

# Email Configuration
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=your_email@example.com
EMAIL_PASS=your_email_password
EMAIL_FROM=no-reply@proxyserver.com

# Payment Gateway
PAYMENT_API_KEY=your_payment_gateway_api_key
PAYMENT_API_SECRET=your_payment_gateway_api_secret

# Proxy Configuration
PROXY_API_ENDPOINT=https://proxy-provider-api.com
PROXY_API_KEY=your_proxy_provider_api_key

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# Admin Configuration
ADMIN_DEFAULT_USERNAME=admin
ADMIN_DEFAULT_PASSWORD=admin_initial_password 